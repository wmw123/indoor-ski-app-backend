{
  "name": "indoor-ski-app-backend",
  "version": "1.0.0",
  "description": "express server with sequelize and pg",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "dev": "env DELAY=1000 npx nodemon .",
    "initdev": "npm run migrate && npm run seed",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "resetDB": "npx sequelize-cli db:migrate:undo:all && npm run initdev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "pg": "^8.2.1",
    "sequelize": "^6.2.4",
    "sequelize-cli": "^6.1.0"
  }
}
